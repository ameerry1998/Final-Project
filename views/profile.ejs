<div class="jumbotron">
  <h1>Your Profile</h1>
  <p> Default data provided by Google Services </p>
</div>
<hr>
<a href="editProfile">Edit</a>
<hr>
<div class="row">
  <div class="col-sm-6">
    <table class="table table-bordered">
      <tbody>
        <tr>
          <td>googlename</td>
          <td>
            <%= user.googlename %>
          </td>
        </tr>
        <tr>
          <td>googleemail</td>
          <td>
            <%= user.googleemail %>
          </td>
        </tr>
        <tr>
          <td>User Type</td>
          <td>
            <%= user.userType %>
          </td>
        </tr>
        <tr>
          <td>username</td>
          <td>
            <%= user.username %>
          </td>
        </tr>
        <tr>
          <td>age</td>
          <td>
            <%= user.age %>
          </td>
        </tr>
      </tbody>
    </table>
  </div>
  <div class="col-sm-6" style="font-size: x-small;">
    <h3>Your search history</h3>
    <hr>
    <% history.forEach(query=> { %>
      <%= query.queryDate %>: <%= query.query %><br>
    <% }) %>
  </div>
</div>